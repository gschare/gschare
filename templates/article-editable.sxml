'(*TOP*
  (template (@ (target "default.sxml"))
   (div (@ (id "modal")
           (class "modal"))
        (div (@ (id "modal-content")
                (class "modal-content"))
             (img (@ (id "modal-image")
                     (class "modal-image")
                     (src "")))))
   (script (@ (src "/js/modal.js")))
   (article (@ (contenteditable "")
               (style "outline: 0px solid transparent"))
       (insert (@ (expr "content"))))))


