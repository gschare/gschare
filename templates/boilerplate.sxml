`(*TOP*
    ,(html->xexp "<!DOCTYPE html>")
    (html (@ (lang "en"))
      (head
        (meta (@ (name "viewport")
                 (content "width=device-width, initial-scale=1.0")))
        (meta (@ (charset "utf-8")))
        (script (@ (src "https://cdn.jsdelivr.net/npm/mathjax@3/es5/tex-chtml-full.js")
                   (type "text/javascript")))
        (template (@ (target "stylesheets.sxml"))))
    (body
      (insert (@ (expr "content"))))))
